    <pet jcr:primaryType="nt:unstructured">
        <components jcr:primaryType="nt:unstructured">
            <form jcr:primaryType="nt:unstructured">
                <container jcr:primaryType="nt:unstructured">
                    <form-container
                        jcr:primaryType="nt:unstructured"
                        jcr:title="pet Form"
                        sling:resourceType="wcm/core/components/policy/policy"
                        components="[/apps/pet/components/form/button,/apps/pet/components/form/hidden,/apps/pet/components/form/options,/apps/pet/components/form/text]">
